{
  "name": "original",
  "version": "1.0.0-beta",
  "private": true,
  "description": "Original theme for projects from scratch.",
  "author": "BA",
  "sasslintConfig": ".",
  "type": "module",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "js": "cross-env NODE_ENV=production webpack --progress",
    "js:zip": "cross-env NODE_ENV=production ZIP=true webpack --progress",
    "watch:js:dev": "cross-env NODE_ENV=development webpack --progress --watch ",
    "watch:theme:dev:win": "cd ./dist && theme open --vars=\"../.env\" && theme watch --vars=\"../.env\"",
    "theme:deploy": "cd ./dist && theme deploy --vars=\"../.env\" --allow-live",
    "theme:open": "cd ./src && theme open --vars=\"../.env\"",
    "build": "cross-env DEPLOY=false npm run js",
    "watch": "npm run watch:theme:dev:win",
    "deploy": "cross-env DEPLOY=true npm run js",
    "start": "cross-env DEPLOY=true && npm run watch:js:dev",
    "start:cli": "cross-env CLI=true npm run watch:js:dev",
    "analyze": "cross-env NODE_ENV=production BUNDLE_ANALIZER=true WEBPACK_PERFORMANCE=true webpack --progress ",
    "zip": "npm run js:zip",
    "lint": "npm run build && theme-check",
    "lint:fix": "npm run build && theme-check -a"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/runtime": "^7.9.6",
    "@fancyapps/ui": "^4.0.5",
    "@shopify/theme-a11y": "^4.0.0",
    "@shopify/theme-addresses": "^3.0.3",
    "@shopify/theme-cart": "^4.0.0",
    "@shopify/theme-currency": "^3.1.0",
    "@shopify/theme-product": "^3.1.0",
    "@shopify/theme-product-form": "^3.1.0",
    "@shopify/theme-sections": "^3.1.0",
    "@splidejs/splide": "^2.4.21",
    "@types/jquery": "^3.5.6",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.21.2",
    "browser-sync": "^2.26.14",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "final-form": "^4.18.5",
    "flatpickr": "^4.6.3",
    "jquery": "^3.2.1",
    "jquery-background-video": "^1.1.0",
    "js-cookie": "^2.2.1",
    "lazysizes": "^5.2.2",
    "modernizr": "^3.6.0",
    "moment": "^2.24.0",
    "objectFitPolyfill": "^2.3.0",
    "picturefill": "^3.0.2",
    "preact": "^10.4.5",
    "redux-zero": "^5.1.7",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "swiper": "^6.5.1",
    "ts-loader": "^8.3.0",
    "typescript": "^4.4.3",
    "velocity-animate": "^1.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.15.6",
    "autoprefixer": "^6.7.2",
    "babel-loader": "^8.0.5",
    "clean-css-cli": "^5.4.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "exports-loader": "^3.0.0",
    "expose-loader": "^0.7.5",
    "filemanager-webpack-plugin": "^6.1.7",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.2",
    "imports-loader": "^3.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss-cli": "^9.0.1",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.55.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  }
}
