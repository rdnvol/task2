{
  "extends": "../tsconfig.json",
  "include": ["../cypress/**/*.ts", "cypress.d.ts"],
  "exclude": [],
  "compilerOptions": {
    "rootDir": "./",
    "types": ["cypress", "@percy/cypress", "node"],
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": ["es2015", "dom"],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true
  }
}
